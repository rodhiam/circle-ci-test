require "bundler/gem_tasks"
require "rspec/core/rake_task"

RSpec::Core::RakeTask.new(:spec)

task :default => :spec

desc "sample for plan"
task :plan do
  puts "plan!!"
end

desc "sample for apply"
task :apply do
  puts "apply!!"
end

desc "sample for failure"
task :failure do
  puts "failure!!"
end
